{
  "name": "snam",
  "version": "1.0.0",
  "private": true,
  "description": "This is my first React-Antd project",
  "scripts": {
    "start": "cross-env NODE_OPTIONS=--openssl-legacy-provider UMI_ENV=dev umi dev",
    "build": "cross-env NODE_OPTIONS=--openssl-legacy-provider max_old_space_size=4096 ANALYZE=1 umi build"
