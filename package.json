    "@uiw/react-amap": "^5.0.0",
    "@uiw/react-codemirror": "^4.20.2",
    "@umijs/plugin-locale": "^0.16.0",
    "ag-grid-community": "28.0.0",
    "ag-grid-react": "28.0.0",
    "ant-design-pro": "^2.3.2",
    "antd": "5.15.4",
    "antd-button-color": "^1.0.4",
